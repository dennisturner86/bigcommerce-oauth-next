{
  "name": "bigcommerce-oauth-next",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/dennisturner86/bigcommerce-oauth-next"
  },
  "main": "dist/index.cjs",
  "bin": {
    "bigcommerce-oauth-next": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build",
    "cli": "tsx src/cli.ts"
  },
  "keywords": [],
  "author": {
    "name": "Dennis Turner",
    "url": "https://github.com/dennisturner86"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "templates"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./use-cases/install": {
      "types": "./dist/use-cases/install/index.d.ts",
      "import": "./dist/use-cases/install/index.js",
      "require": "./dist/use-cases/install/index.cjs"
    },
    "./use-cases/load": {
      "types": "./dist/use-cases/load/index.d.ts",
      "import": "./dist/use-cases/load/index.js",
      "require": "./dist/use-cases/load/index.cjs"
    },
    "./use-cases/uninstall": {
      "types": "./dist/use-cases/uninstall/index.d.ts",
      "import": "./dist/use-cases/uninstall/index.js",
      "require": "./dist/use-cases/uninstall/index.cjs"
    },
    "./use-cases/remove-user": {
      "types": "./dist/use-cases/remove-user/index.d.ts",
      "import": "./dist/use-cases/remove-user/index.js",
      "require": "./dist/use-cases/remove-user/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "next": "^16.0.7",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "bigcommerce-oauth": "^1.0.2"
  },
  "peerDependencies": {
    "next": ">=15"
  }
}
